<form action="/admin/menus" method="post">
    <input type="hidden" name="id" value="<%= _data.menuItems.id %>">
    <div class="form-group row">
        <label for="restaurant_id" class="col-sm-2 col-form-label">Restaurant Id</label>
        <div class="col-sm-10">
            <input class="form-control#restaurant_id" type="text" name="restaurant_id" value="<%= _data.menuItems.restaurant_id %>">
        </div>
    </div>

    <div class="form-group row">
        <label for="item_name" class="col-sm-2 col-form-label">Item Name</label>
        <div class="col-sm-10">
            <input class="form-control#item_name" type="text" name="item_name" value="<%= _data.menuItems.item_name %>">
        </div>
    </div>

    <div class="form-group row">
        <label for="item_price" class="col-sm-2 col-form-label">Item Price</label>
        <div class="col-sm-10">
            <input class="form-control#item_price" type="text" name="item_price" value="<%= _data.menuItems.item_price %>">
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10 offset-sm-2">
            <input class="btn btn-primary" type="submit" value="Save Restaurant">
        </div>
    </div>

</form>